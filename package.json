{
  "name": "goclassshow",
  "displayName": "go类图展示",
  "description": "",
  "publisher": "peng456",
  "author": "peng456",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/peng456/go-calss-uml-vs.git"
  },
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "files": [
    "dist",
    "media",
    "out",
    "package.json",
    "README.md"
  ],
  "contributes": {
    "languages": [
      {
        "id": "go",
        "extensions": [
          ".go"
        ],
        "aliases": [
          "Go"
        ]
      }
    ],
    "commands": [
      {
        "command": "goclassshow.showgoclassdiagram",
        "when": "editorTextFocus  && editorLangId == 'go'",
        "title": "展示go类图"
      },
      {
        "command": "goclassshow.gitdiagram_generate",
        "when": "editorTextFocus  && editorLangId == 'go'",
        "title": "展示架构图"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "goclassshow.showgoclassdiagram",
          "when": "editorTextFocus  && editorLangId == 'go'",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "command": "goclassshow.showgoclassdiagram",
          "when": "editorTextFocus && editorLangId == 'go'",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "command": "goclassshow.showgoclassdiagram",
          "when": "editorTextFocus && editorLangId == 'go'",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "goclassshow.showgoclassdiagram",
        "key": "shift+a+s",
        "mac": "shift+a+s",
        "when": "editorTextFocus && editorLangId == 'go'"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.7.2"
  }
}
